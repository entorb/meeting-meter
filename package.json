{
  "name": "meeting-meter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.19.0 <23 || >=24.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "prepare": "cypress install",
    "test": "vitest --watch=true",
    "test-once": "vitest --watch=false --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ cypress/ *.ts *.json",
    "check": "run-p lint format type-check"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.8",
    "@mdi/js": "^7.4.47",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.4"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.6.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.15",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.3.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-vuetify": "^4.2.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "~10.5.0",
    "eslint": "^9.37.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.3",
    "unplugin-vue-components": "^29.1.0",
    "unplugin-vue-router": "^0.15.0",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vite-plugin-vuetify": "^2.1.2",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
