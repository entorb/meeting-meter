{
  "name": "meeting-meter",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.19.0 <23 || >=24.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "run": "run-s build preview",
    "test": "vitest --watch=false",
    "test-cov": "vitest --watch=false --coverage",
    "cy:run": "cypress run --e2e",
    "cy:open": "cypress open --e2e",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --cache --fix --ignore-pattern .gitignore",
    "format": "prettier --cache --write src/ cypress/ *.html *.ts *.json *.md --log-level silent",
    "spell": "cspell --cache --quiet \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\"",
    "spell-store": "cspell --cache --quiet --words-only --unique \"src/**/*.{txt,md,html,js,ts,vue,json,yml,py}\" > cspell-unknown-words.txt",
    "check": "run-p format lint type-check spell test"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.9",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "vuetify": "^3.11.7",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.9",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cspell": "^9.6.0",
    "cypress": "^15.9.0",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "~10.5.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "sass-embedded": "^1.97.3",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3",
    "unplugin-vue-components": "^29.2.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.3"
  }
}
